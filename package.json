    "format": "deno fmt src",
    "lint": "pnpm lint:ts",
    "lint:ts": "deno lint src",
    "lint:types": "tsc --noEmit",
    "fix:ts": "deno lint src --fix",
    "build_": "tsdown && vite build",
    "build": "tsdown",
    "prepublishOnly": "pnpm update --latest && pnpm update -r && pnpm format && pnpm lint && pnpm build && pnpm badges",
